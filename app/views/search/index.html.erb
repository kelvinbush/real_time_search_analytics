<h1>Users with what they searched for</h1><% if @searches.count > 0 %>  <table class="table table-sm">    <thead>    <th>Author</th>    <th>Query</th>    </thead>    <tbody>    <% @searches.each do |search| %>      <tr>        <td><%= search.author.name %></td>        <td> <%= search.text %></td>      </tr>    <% end %>    </tbody>  </table><% else %>  <h3>You have no analytics, kindly for articles first</h3><% end %>